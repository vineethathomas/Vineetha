package com.string.sorting.config;

import com.string.sorting.service.SortService;
import com.string.sorting.service.SortServiceImpl;
import org.springframework.context.annotation.*;

@Configuration
public class AppConfig {
 
    @Bean(name="helloWorldBean")
    @Description("This is a sample HelloWorld Bean")
    public SortService helloWorld() {
        return new SortServiceImpl();
    }
 
}